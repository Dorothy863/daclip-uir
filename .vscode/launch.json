    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "test",
            "type": "debugpy",
            "request": "launch",
            "program": "/workspace/daclip-uir/universal_image_restoration/config/daclip-sde/test.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-opt=/workspace/daclip-uir/universal_image_restoration/config/daclip-sde/options/test.yml"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            }
        }
    ]
}